{{page-title "Les Miss"}}
<h2>Toutes les miss</h2>
<ul class="misses">
  {{#each this.model as |miss|}}
  <li class="miss">
    <LinkTo @route="miss.profile" @model={{ miss }}>
      <div class="portrait">
        <img src={{miss.photos.portrait}} alt="" role="none">
      </div>
      <section class="legend">
        <h3 class="region">{{miss.r√©gion}}</h3>
        <h4 class="nom">{{miss.nom}}</h4>
      </section>
    </LinkTo>
  </li>
  {{/each}}
</ul>
{{outlet}}
